{
  "name": "react-sketch-canvas",
  "version": "5.3.1",
  "root": true,
  "description": "react-sketch-canvas - Freehand vector drawing tool for React using SVG as canvas",
  "source": "src/index.ts",
  "main": "dist/react-sketch-canvas.js",
  "umd:main": "dist/react-sketch-canvas.umd.js",
  "module": "dist/react-sketch-canvas.m.js",
  "unpkg": "dist/react-sketch-canvas.umd.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "yarn clean && microbundle --jsx React.createElement --globals react=React",
    "clean": "rimraf dist",
    "start": "yarn clean && microbundle watch --jsx React.createElement --globals react=React"
  },
  "dependencies": {
    "immer": "^8.0.1",
    "pepjs": "^0.5.2"
  },
  "peerDependencies": {
    "react": "16.x"
  },
  "devDependencies": {
    "eslint": "^7.10.0",
    "eslint-config-airbnb-typescript-prettier": "^3.1.0",
    "lerna": "^3.22.1",
    "microbundle": "^0.12.4",
    "prettier": "^2.1.2",
    "react": "^16.13.1",
    "rimraf": "^3.0.2"
  },
  "author": "Vinoth Pandian",
  "homepage": "https://vinoth.info/react-sketch-canvas",
  "license": "MIT",
  "repository": "https://github.com/vinothpandian/react-sketch-canvas.git",
  "keywords": [
    "react-component",
    "sketch",
    "canvas",
    "drawing",
    "freehand",
    "vector",
    "svg-canvas",
    "react-sketch"
  ],
  "files": [
    "dist/**.js*",
    "dist/**/*.ts"
  ]
}
